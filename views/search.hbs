

<center>
<div class="container">
<div class="col-sm-6 col-md-6">
<div class='formbg'>
<h3>Search</h3>

<form class="" action="" method="GET">
    <label>Search for Bookmarks by User:</label>

  <div class="form-group">
  <input class="form-control" type="text" name="search" value="" required>
</div>
  <input class="btn btn-primary" type="submit" value="Search">
</form>

</div>

</div>


  {{#if bookmark}}
    <div class="display-4">Results for {{user}}: </div>
      <div class="bookmarkContainer">
    {{#each bookmark as |b|}}

      <a href='{{b.url}}'> <h1>{{b.name}} {{b.none}}</h1></a>
    {{/each}}
        </div>
  {{/if}}




    {{#if none}}
      <div class='flash display-4'>{{none}} </div>
    {{/if}}

</div>
</center>
