

<center>
<div class="container">
<div class="col-sm-6 col-md-6">
<div class='formbg'>
<h3>Search</h3>

<form class="" action="" method="GET">
    <label>Search for Bookmark by User:</label>

  <div class="form-group">
  <input class="form-control" type="text" name="search" value="" required>
</div>
  <input class="btn btn-primary" type="submit" value="Search">
</form>

</div>

{{#if bookmark}}
  <h3>Results: </h3>
  {{#each bookmark as |b|}}
    <a href='{{b.url}}'> <h1>{{b.name}} {{b.none}}</h1></a>
  {{/each}}
{{/if}}

{{#if none}}
  <h3>{{none}} </h3>
{{/if}}

</div>
</div>
</center>
